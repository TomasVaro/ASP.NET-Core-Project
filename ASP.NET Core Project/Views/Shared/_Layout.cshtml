<!DOCTYPE html>

<html>
<head>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js" integrity="sha384-smHYKdLADwkXOn1EmN1qk/HfnUcbVRZyYmZ4qpPea6sjB/pTJ0euyQp0Mk8ck+5T" crossorigin="anonymous"></script>
    <script src="PersonScript.js" defer></script>
    <meta name="viewport" content="width=device-width" />
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <title>@ViewBag.Title</title>
</head>
<body>
    <header>
        <h1>Tomas homepage</h1>
        <div class="menu">
            <a asp-controller="Home" asp-action="Index">Home</a>
            <a asp-controller="Home" asp-action="About">About</a>
            <a asp-controller="Home" asp-action="Contact">Contact</a>
            <a asp-controller="Home" asp-action="Projects">Projects</a>
            <a asp-controller="Doctor" asp-action="FeverCheck">FeverCheck</a>
            <a asp-controller="Game" asp-action="NumberGuessing">NumberGame</a>
            <a asp-controller="People" asp-action="ListOfPeople">ListPeople</a>
            <a asp-controller="Ajax" asp-action="Index">ListPeopleWithAjax</a>
        </div>
    </header>
    <main>
        <div class="container">
            @RenderBody()
        </div>
    </main>
    <footer>
        <br /><br />
        @DateTime.Now.ToShortDateString() Tomas Varo
    </footer>
</body>
</html>
